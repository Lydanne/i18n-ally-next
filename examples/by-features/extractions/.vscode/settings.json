{
  "i18n-ally-next.localesPaths": "locales",
  "i18n-ally-next.sourceLanguage": "en",
  "i18n-ally-next.keystyle": "nested",
  "i18n-ally-next.extract.autoDetect": true,
  "i18n-ally-next.editor.preferEditor": true,
  "i18n-ally-next.translate.saveAsCandidates": true,
  "i18n-ally-next.refactor.templates": [
    {
      "source": "html-attribute",
      "template": "z('{key}'{args})"
    },
    {
      "template": "ttt('{key}')",
      "include": [
        "Demo.vue"
      ]
    }
  ],
  "i18n-ally-next.extract.ignored": [
    "Hello"
  ],
  "i18n-ally-next.extract.ignoredByFiles": {
    "Demo.vue": [
      "Demo"
    ],
    "nested/a.tsx": [
      "World"
    ]
  }
}

{
  "publisher": "lydanne",
  "name": "i18n-ally-next",
  "displayName": "i18n Ally Next",
  "version": "3.0.0",
  "packageManager": "pnpm@9.15.4",
  "description": "üåç All in one i18n extension for VS Code",
  "homepage": "https://lydanne.github.io/i18n-ally-next/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lydanne/i18n-ally-next"
  },
  "bugs": "https://github.com/lydanne/i18n-ally-next/issues",
  "keywords": [
    "vue",
    "vue-i18n",
    "translate",
    "i18n",
    "i18next"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/extension",
  "icon": "res/logo.png",
  "engines": {
    "vscode": "^1.106.0"
  },
  "activationEvents": [
    "workspaceContains:package.json",
    "workspaceContains:pubspec.yaml",
    "workspaceContains:composer.json",
    "workspaceContains:Gemfile",
    "workspaceContains:.vscode/i18n-ally-next-custom-framework.yml",
    "workspaceContains:.vscode/i18n-ally-next-reviews.yml",
    "workspaceContains:*.vue",
    "onWebviewPanel:i18n-ally-next-editor",
    "onLanguage:vue",
    "onLanguage:html",
    "onLanguage:pug",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:yaml",
    "onLanguage:json5",
    "onLanguage:dart",
    "onLanguage:php",
    "onLanguage:handlebars",
    "onLanguage:ini",
    "onLanguage:svelte"
  ],
  "contributes": {
    "commands": [
      {
        "command": "i18n-ally-next.config-locales",
        "title": "%command.config_locales%",
        "category": "%extname%"
      },
      {
        "command": "i18n-ally-next.config-locales-auto",
        "title": "%command.config_locales_auto%",
        "category": "%extname%"
      },
      {
        "command": "i18n-ally-next.config-display-language",
        "title": "%command.config_display_language%",
        "category": "%extname%"
      },
      {
        "command": "i18n-ally-next.config-source-language",
        "title": "%command.config_source_language%",
        "category": "%extname%"
      },
      {
        "command": "i18n-ally-next.set-display-language",
        "title": "%command.set_display_language%",
        "category": "%extname%"
      },
      {
        "command": "i18n-ally-next.set-source-language",
        "title": "%command.set_source_language%",
        "category": "%extname%"
      },
      {
        "command": "i18n-ally-next.copy-key",
        "category": "%extname%",
        "title": "%command.copy_key%",
        "icon": "$(clippy)"
      },
      {
        "command": "i18n-ally-next.translate-key",
        "category": "%extname%",
        "title": "%command.translate_key%",
        "icon": "$(globe)"
      },
      {
        "command": "i18n-ally-next.edit-key",
        "category": "%extname%",
        "title": "%command.edit_key%",
        "icon": "$(edit)"
      },
      {
        "command": "i18n-ally-next.open-key",
        "category": "%extname%",
        "title": "%command.open_key%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "i18n-ally-next.delete-key",
        "category": "%extname%",
        "title": "%command.delete_key%"
      },
      {
        "command": "i18n-ally-next.rename-key",
        "category": "%extname%",
        "title": "%command.rename_key%"
      },
      {
        "command": "i18n-ally-next.extract-text",
        "category": "%extname%",
        "title": "%refactor.extract_text%",
        "icon": "$(symbol-string)"
      },
      {
        "command": "i18n-ally-next.extract-hard-strings-batch",
        "category": "%extname%",
        "title": "Extract all hard-coded strings (experimental)"
      },
      {
        "command": "i18n-ally-next.detect_hard_strings",
        "category": "%extname%",
        "title": "Detect hard-coded strings in current file (experimental)"
      },
      {
        "command": "i18n-ally-next.open-url",
        "category": "%extname%",
        "title": "%command.open_url%"
      },
      {
        "command": "i18n-ally-next.fulfill-keys",
        "category": "%extname%",
        "title": "%command.fulfill_keys%",
        "icon": "$(selection)"
      },
      {
        "command": "i18n-ally-next.refresh-usage",
        "category": "%extname%",
        "title": "%command.refresh_usage%",
        "icon": "$(refresh)"
      },
      {
        "command": "i18n-ally-next.support",
        "category": "%extname%",
        "title": "%feedback.support%"
      },
      {
        "command": "i18n-ally-next.locale-visibility-show",
        "category": "%extname%",
        "title": "%command.locale_visibility_show%",
        "icon": "$(eye)"
      },
      {
        "command": "i18n-ally-next.locale-visibility-hide",
        "category": "%extname%",
        "title": "%command.locale_visibility_hide%",
        "icon": "$(eye-closed)"
      },
      {
        "command": "i18n-ally-next.new-key",
        "category": "%extname%",
        "title": "%command.new_key%",
        "icon": "$(plus)"
      },
      {
        "command": "i18n-ally-next.duplicate-key",
        "category": "%extname%",
        "title": "%command.duplicate_key%"
      },
      {
        "command": "i18n-ally-next.mark-key-as-in-use",
        "category": "%extname%",
        "title": "%command.mark_key_as_in_use%"
      },
      {
        "command": "i18n-ally-next.open-in-editor",
        "category": "%extname%",
        "title": "%command.open_in_editor%",
        "icon": "$(file-symlink-file)"
      },
      {
        "command": "i18n-ally-next.open-editor",
        "category": "%extname%",
        "title": "%command.open_editor%"
      },
      {
        "command": "i18n-ally-next.review.comment",
        "category": "%extname%",
        "title": "%review.leave_comment%",
        "enablement": "!commentIsEmpty"
      },
      {
        "command": "i18n-ally-next.review.approve",
        "category": "%extname%",
        "title": "%review.approve%"
      },
      {
        "command": "i18n-ally-next.review.request-change",
        "category": "%extname%",
        "title": "%review.request_change%"
      },
      {
        "command": "i18n-ally-next.review.edit",
        "category": "%extname%",
        "title": "%review.edit%",
        "icon": "$(edit)"
      },
      {
        "command": "i18n-ally-next.review.resolve",
        "title": "%review.resolve%",
        "icon": "$(issue-closed)"
      },
      {
        "command": "i18n-ally-next.review.resolve-thread",
        "category": "%extname%",
        "title": "%review.resolve_all%",
        "icon": "$(checklist)"
      },
      {
        "command": "i18n-ally-next.review.apply-translation",
        "category": "%extname%",
        "title": "%review.apply_translation_candidate%",
        "icon": "$(check)"
      },
      {
        "command": "i18n-ally-next.review.apply-suggestion",
        "category": "%extname%",
        "title": "%review.apply_suggestion%"
      },
      {
        "command": "i18n-ally-next.insert-key",
        "category": "%extname%",
        "title": "%command.insert_key%"
      },
      {
        "command": "i18n-ally-next.deepl-usage",
        "category": "%extname%",
        "title": "%command.deepl_usage%"
      },
      {
        "command": "i18n-ally-next.go-to-range",
        "category": "%extname%",
        "title": "%command.go_to_range%"
      },
      {
        "command": "i18n-ally-next.go-to-next-usage",
        "category": "%extname%",
        "title": "%command.go_to_next_usage%"
      },
      {
        "command": "i18n-ally-next.go-to-prev-usage",
        "category": "%extname%",
        "title": "%command.go_to_prev_usage%"
      },
      {
        "command": "i18n-ally-next.open-docs-hard-string",
        "category": "%extname%",
        "title": "%command.show_docs%",
        "icon": "$(question)"
      },
      {
        "command": "i18n-ally-next.extract-disable-auto-detect",
        "category": "%extname%",
        "title": "%command.extract.disable-auto-detect%"
      },
      {
        "command": "i18n-ally-next.extract-enable-auto-detect",
        "category": "%extname%",
        "title": "%command.extract.enable-auto-detect%"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "i18n-ally-next.set-display-language",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.set-source-language",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.copy-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.edit-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.delete-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.rename-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.translate-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.open-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.open-url",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.locale-visibility-hide",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.locale-visibility-show",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.mark-key-as-in-use",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.comment",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.approve",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.request-change",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.resolve-thread",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.resolve",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.apply-translation",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.review.apply-suggestion",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.open-in-editor",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.edit-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.duplicate-key",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.go-to-range",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.extract-disable-auto-detect",
          "when": "false"
        },
        {
          "command": "i18n-ally-next.extract-enable-auto-detect",
          "when": "false"
        }
      ],
      "view/item/context": [
        {
          "command": "i18n-ally-next.open-in-editor",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /open-in-editor/",
          "group": "inline@1"
        },
        {
          "command": "i18n-ally-next.open-key",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /openable/",
          "group": "inline@2"
        },
        {
          "command": "i18n-ally-next.copy-key",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /node/",
          "group": "inline@3"
        },
        {
          "command": "i18n-ally-next.translate-key",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /translatable/",
          "group": "inline@4"
        },
        {
          "command": "i18n-ally-next.edit-key",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /editable/",
          "group": "inline@5"
        },
        {
          "command": "i18n-ally-next.fulfill-keys",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /fulfillable/",
          "group": "inline@3"
        },
        {
          "command": "i18n-ally-next.review.apply-translation",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /translation-candidate/",
          "group": "inline@1"
        },
        {
          "command": "i18n-ally-next.set-display-language",
          "when": "view =~ /i18n-ally-next-locales-progress/ && viewItem =~ /notdisply/"
        },
        {
          "command": "i18n-ally-next.set-source-language",
          "when": "view =~ /i18n-ally-next-locales-progress/ && viewItem =~ /notsource/"
        },
        {
          "command": "i18n-ally-next.rename-key",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /node/ && viewItem =~ /writable/",
          "group": "i18nally@1"
        },
        {
          "command": "i18n-ally-next.duplicate-key",
          "when": "view =~ /i18n-ally-next-locales-tree/ && viewItem =~ /node/ && viewItem =~ /writable/",
          "group": "i18nally@2"
        },
        {
          "command": "i18n-ally-next.delete-key",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /node/ && viewItem =~ /writable/",
          "group": "i18nally@3"
        },
        {
          "command": "i18n-ally-next.locale-visibility-show",
          "when": "view =~ /i18n-ally-next-locales-progress/ && viewItem =~ /show/",
          "group": "inline@4"
        },
        {
          "command": "i18n-ally-next.locale-visibility-hide",
          "when": "view =~ /i18n-ally-next-locales-progress/ && viewItem =~ /hide/",
          "group": "inline@4"
        },
        {
          "command": "i18n-ally-next.delete-key",
          "when": "view =~ /i18n-ally-next-locales-usage/ && viewItem =~ /usage_idle/",
          "group": "i18nally@1"
        },
        {
          "command": "i18n-ally-next.mark-key-as-in-use",
          "when": "view =~ /i18n-ally-next-locales/ && viewItem =~ /key_idle/",
          "group": "i18nally@1"
        },
        {
          "command": "i18n-ally-next.open-docs-hard-string",
          "when": "viewItem =~ /i18n-ally-next-hard-string-root/",
          "group": "inline@1"
        },
        {
          "command": "i18n-ally-next.extract-text",
          "when": "viewItem =~ /i18n-ally-next-hard-string-item/",
          "group": "inline@1"
        },
        {
          "command": "i18n-ally-next.extract-hard-strings-batch",
          "when": "viewItem =~ /i18n-ally-next-hard-string-root/",
          "group": "i18nally@1"
        },
        {
          "command": "i18n-ally-next.extract-disable-auto-detect",
          "when": "viewItem =~ /i18n-ally-next-hard-string-root/ && i18n-ally-next.extract.autoDetect == true",
          "group": "i18nally@2"
        },
        {
          "command": "i18n-ally-next.extract-enable-auto-detect",
          "when": "viewItem =~ /i18n-ally-next-hard-string-root/ && i18n-ally-next.extract.autoDetect == false",
          "group": "i18nally@2"
        }
      ],
      "view/title": [
        {
          "command": "i18n-ally-next.refresh-usage",
          "group": "navigation",
          "when": "view =~ /i18n-ally-next-locales-usage/"
        },
        {
          "command": "i18n-ally-next.new-key",
          "group": "navigation",
          "when": "view =~ /i18n-ally-next-locales-tree/"
        }
      ],
      "explorer/context": [
        {
          "command": "i18n-ally-next.extract-hard-strings-batch",
          "group": "1_i18nally@1"
        }
      ],
      "comments/commentThread/title": [
        {
          "command": "i18n-ally-next.review.resolve-thread",
          "group": "navigation",
          "when": "commentController == i18n-ally-next-review && !commentThreadIsEmpty"
        }
      ],
      "comments/commentThread/context": [
        {
          "command": "i18n-ally-next.review.approve",
          "group": "inline@1",
          "when": "commentController == i18n-ally-next-review"
        },
        {
          "command": "i18n-ally-next.review.request-change",
          "group": "inline@2",
          "when": "commentController == i18n-ally-next-review"
        },
        {
          "command": "i18n-ally-next.review.comment",
          "group": "inline@3",
          "when": "commentController == i18n-ally-next-review"
        }
      ],
      "comments/comment/title": [
        {
          "command": "i18n-ally-next.review.edit",
          "group": "group@1",
          "when": "commentController == i18n-ally-next-review && viewItem =~ /self/"
        },
        {
          "command": "i18n-ally-next.review.resolve",
          "group": "group@2",
          "when": "commentController == i18n-ally-next-review"
        }
      ],
      "comments/comment/context": []
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "i18n-ally-next",
          "title": "%extname%",
          "icon": "./res/logo-view-container.svg"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "i18n-ally-next-locales-file-explorer",
          "name": "%view.i18n_keys%",
          "when": "i18n-ally-next-enabled"
        }
      ],
      "i18n-ally-next": [
        {
          "id": "i18n-ally-next-locales-sidebar-file",
          "name": "%view.current_file%",
          "when": "i18n-ally-next-enabled"
        },
        {
          "id": "i18n-ally-next-locales-progress",
          "name": "%view.progress%",
          "when": "i18n-ally-next-enabled"
        },
        {
          "id": "i18n-ally-next-locales-tree",
          "name": "%view.tree%",
          "when": "i18n-ally-next-enabled"
        },
        {
          "id": "i18n-ally-next-locales-usage",
          "name": "%view.usage%",
          "when": "i18n-ally-next-enabled"
        },
        {
          "id": "i18n-ally-next-help-feedback",
          "name": "%view.help_feedback%",
          "when": "i18n-ally-next-enabled"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "%extname%",
      "properties": {
        "i18n-ally-next.disabled": {
          "type": "boolean",
          "default": false,
          "description": "%config.disabled%"
        },
        "i18n-ally-next.autoDetection": {
          "type": "boolean",
          "default": true,
          "description": "%config.auto_detection%"
        },
        "i18n-ally-next.localesPaths": {
          "type": [
            "string",
            "array"
          ],
          "items": {
            "type": "string"
          },
          "description": "%config.locales_paths%",
          "scope": "resource"
        },
        "i18n-ally-next.encoding": {
          "type": "string",
          "default": "utf-8",
          "description": "%config.encoding%"
        },
        "i18n-ally-next.sourceLanguage": {
          "type": "string",
          "description": "%config.source_language%"
        },
        "i18n-ally-next.displayLanguage": {
          "type": "string",
          "description": "%config.display_language%"
        },
        "i18n-ally-next.ignoredLocales": {
          "type": "array",
          "description": "%config.ignored_locales%"
        },
        "i18n-ally-next.keystyle": {
          "type": "string",
          "enum": [
            "auto",
            "nested",
            "flat"
          ],
          "description": "%config.keystyle%"
        },
        "i18n-ally-next.dirStructure": {
          "type": "string",
          "enum": [
            "auto",
            "file",
            "dir"
          ],
          "description": "%config.dir_structure%"
        },
        "i18n-ally-next.annotations": {
          "type": "boolean",
          "default": true,
          "description": "%config.annotations%"
        },
        "i18n-ally-next.annotationInPlace": {
          "type": "boolean",
          "default": true,
          "description": "%config.annotation_in_place%"
        },
        "i18n-ally-next.annotationMaxLength": {
          "type": "number",
          "default": 40,
          "description": "%config.annotation_max_length%"
        },
        "i18n-ally-next.annotationDelimiter": {
          "type": "string",
          "default": "¬∑",
          "description": "%config.annotation_delimiter%"
        },
        "i18n-ally-next.includeSubfolders": {
          "type": "boolean",
          "default": true,
          "description": "%config.include_subfolders%"
        },
        "i18n-ally-next.fullReloadOnChanged": {
          "type": "boolean",
          "default": false,
          "description": "%config.full_reload_on_changed%"
        },
        "i18n-ally-next.showFlags": {
          "type": "boolean",
          "default": true,
          "description": "%config.show_flags%"
        },
        "i18n-ally-next.enabledFrameworks": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "vue",
              "react",
              "vscode",
              "ngx-translate",
              "i18next",
              "react-i18next",
              "i18next-shopify",
              "i18n-tag",
              "flutter",
              "vue-sfc",
              "ember",
              "chrome-ext",
              "ruby-rails",
              "custom",
              "laravel",
              "transloco",
              "svelte",
              "globalize",
              "ui5",
              "next-translate",
              "php-gettext",
              "general",
              "lingui",
              "jekyll",
              "fluent-vue",
              "fluent-vue-sfc",
              "next-intl",
              "next-international"
            ]
          },
          "description": "%config.enabled_frameworks%"
        },
        "i18n-ally-next.enabledParsers": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "js",
              "ts",
              "json",
              "json5",
              "yaml",
              "ini",
              "po",
              "php",
              "properties",
              "ftl"
            ]
          },
          "description": "%config.enabled_parsers%"
        },
        "i18n-ally-next.keysInUse": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "%config.keys_in_use%"
        },
        "i18n-ally-next.sortKeys": {
          "type": "boolean",
          "default": false,
          "description": "%config.sort_keys%"
        },
        "i18n-ally-next.sortCompare": {
          "type": "string",
          "enum": [
            "binary",
            "locale"

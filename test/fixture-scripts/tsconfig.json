{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ESNext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "~/*": ["../../dist/src/*"]
    },
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "../fixture-scripts-out",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "../../src/",
    "../../dist/"
  ]
}
